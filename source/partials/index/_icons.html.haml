%ul.icons
  %li.icon
    .tooltip Email
    %a.circle.tooltip-target{ href: "mailto:#{data.me.email}", target: '_blank' }
      %span.icon-envelope-o

  %li.icon
    .tooltip Github
    %a.circle.tooltip-target{ href: data.services.github.url, target: '_blank' }
      %span.icon-github-alt

  %li.icon
    .tooltip LinkedIn
    %a.circle.tooltip-target{ href: data.services.linkedin.url, target: '_blank' }
      %span.icon-linkedin
